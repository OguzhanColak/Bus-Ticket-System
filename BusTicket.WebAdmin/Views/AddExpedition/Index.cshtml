@model BusTicket.WebAdmin.Models.GridPanelModel
@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <title>
        SegmentedButton Variations - Ext.NET Examples
    </title>
    <link href="/resources/css/examples.css" rel="stylesheet" />
</head>
<body>
    <h1>
        Sefer Ekle
    </h1>
    
    <ext-container layout="Form">
        <items>
            <ext-fieldContainer fieldLabel="Otobüsün kalkacağı il">
                <items>
                    <ext-segmentedButton allowToggle="false">
                        <items>
                            <ext-comboBox id="selam">
                                @*<DirectEvents>
                                    <Select>
                                        <EventMask ShowMask="true"></EventMask>
                                    </Select>
                                </DirectEvents>*@
                                <Items>
                                    <ext-listItem Value="0" Text="Edirne">
                                    <ext-listItem Value="1" Text="İstanbul">
                                    <ext-listItem Value="2" Text="Ankara">
                                </Items>
                            </ext-comboBox>
                        </items>
                    </ext-segmentedButton>
                </items>
            </ext-fieldContainer>

            <ext-fieldContainer fieldLabel="Otobüsün varacağı il">
                <items>
                    <ext-segmentedButton allowToggle="false">
                        <items>
                            <ext-comboBox>
                                <DirectEvents>
                                    <Select>
                                        <EventMask ShowMask="true"></EventMask>
                                    </Select>
                                </DirectEvents>
                                <Items>
                                    <ext-listItem Value="0" Text="Edirne">
                                    <ext-listItem Value="1" Text="İstanbul">
                                    <ext-listItem Value="2" Text="Ankara">
                                </Items>
                            </ext-comboBox>
                        </items>
                    </ext-segmentedButton>
                </items>
            </ext-fieldContainer>
        </items>

    </ext-container>





</body>
</html>




